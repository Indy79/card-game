{
  "name": "card-game",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "install": "npm run build",
    "start": "nodemon --exec babel-node src/server/index.js --presets env",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:client": "parcel watch src/client/index.html --out-dir dist/client",
    "dev:server": "nodemon --watch ./src/server --exec babel-node ./src/server/index.js --presets env",
    "build:client": "parcel build src/client/index.html --out-dir dist/client",
    "build:server": "babel src/server -s -d dist/server",
    "build": "npm run build:client",
    "build:start": "npm run build && npm start",
    "dev": "concurrently --kill-others \"npm run dev:server\" \"npm run dev:client\""
  },
  "engines": {
    "node": ">=10"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^5.1.0",
    "parcel-bundler": "^1.12.4"
  },
  "dependencies": {
    "antd": "^4.1.4",
    "app-root-path": "^3.0.0",
    "card-deck": "^1.0.1",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "nodemon": "^2.0.3",
    "playing-cards-js": "0.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-playing-card": "^0.2.5",
    "react-router-dom": "^5.1.2",
    "rxjs": "^6.5.5",
    "socket.io": "^2.3.0"
  }
}
